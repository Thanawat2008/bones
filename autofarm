end)

MainSection1:CreateToggle("Blox Fruit", function(z)
_G.Point = z
while _G.Point do wait()
    local args = {
        [1] = "AddPoint",
        [2] = "Blox Fruit",
        [3] = point
    }

    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
